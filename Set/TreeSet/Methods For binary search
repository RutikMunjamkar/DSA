TreeSet<Integer> set = new TreeSet<>();
set.add(2);
set.add(4);
set.add(6);
set.add(8);


floor(key)	--- Greatest element ≤ given key	--->> set.floor(5)----->>	4
ceiling(key) --	Smallest element ≥ given key	--->> set.ceiling(5)--->>	6
lower(key)	--- Greatest element < given key	--->> set.lower(5)	--->>   4
higher(key)	--- Smallest element > given key	--->> set.higher(5)	--->>   6
